  },
  "author": "Shawn Bernard <casualbotdev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CasualBot/jest-sonar-reporter/issues"
  },
  "homepage": "https://github.com/CasualBot/jest-sonar-reporter#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-modules-commonjs": "7.16.0",
    "@babel/plugin-transform-typescript": "7.16.1",
    "@babel/preset-env": "^.16.4",
    "@babel/preset-typescript": "7.16.0",
    "@types/jest": "27.0.3",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "16.11.12",
    "@types/uuid": "^8.3.3",
    "@types/xml": "1.0.6",
    "babel-jest": "27.4.2",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.3",
    "prettier": "2.5.1",
    "strip-ansi": "^7.0.1",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.2",
    "uuid": "8.3.2"
