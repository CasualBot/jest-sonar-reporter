{
  "name": "jest-sonar-reporter",
  "version": "3.0.0",
  "description": "A Sonar test reporter for Jest.",
  "keywords": [
    "sonar",
    "sonarqube",
    "jest",
    "reporter",
    "processor"
  ],
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CasualBot/jest-sonar-reporter.git"
  },
  "author": "Shawn Bernard <casualbotdev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CasualBot/jest-sonar-reporter/issues"
  },
  "homepage": "https://github.com/CasualBot/jest-sonar-reporter#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.3",
    "prettier": "2.5.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "dependencies": {
    "xml": "1.0.1"
  }
}
